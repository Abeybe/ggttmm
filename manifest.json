{
    "name": "ggttmm",
    "version": "1.0",
    "description": "ぐぐってメモ",
    "permissions": [
      "declarativeContent", "storage",
      "contextMenus","tabs","file:///*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.google.com/*"],
        "js": [
          "jquery.min.js",
          "ggttmm.js"
          ],
          "css":["myStyle.css"],
          "run_at": "document_start"
      }
    ],
    "page_action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources":["source/ggttmm.png"],
    "background": {
      "scripts": ["background.js"]
      
    },
    "manifest_version": 2
}